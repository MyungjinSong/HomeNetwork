<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config>
    <webserver>
        <host>0.0.0.0</host>
        <port>1234</port>
        <log>1</log>
    </webserver>
    <rs485>
        <reconnect_limit>30</reconnect_limit>  <!-- 단위: 초 -->
        <light>
            <enable>1</enable>
            <type>1</type>  <!-- 0 = usb2serial, 1 = ew11 -->
            <usb2serial>
                <port>/dev/ttyUSB1</port>
                <baud>9600</baud>
            </usb2serial>
            <ew11>
                <ipaddr>192.168.0.1</ipaddr>
                <port>8899</port>
            </ew11>
        </light>
        <various>
            <enable>1</enable>
            <type>1</type>  <!-- 0 = usb2serial, 1 = ew11 -->
            <usb2serial>
                <port>/dev/ttyUSB0</port>
                <baud>9600</baud>
            </usb2serial>
            <ew11>
                <ipaddr>192.168.0.2</ipaddr>
                <port>8899</port>
            </ew11>
        </various>
        <doorphone>
            <enable>0</enable>
            <type>0</type>  <!-- 0 = usb2serial, 1 = ew11 -->
            <usb2serial>
                <port>/dev/ttyUSB0</port>
                <baud>9600</baud>
            </usb2serial>
            <ew11>
                <ipaddr>192.168.0.1</ipaddr>
                <port>8899</port>
            </ew11>
        </doorphone>
        <subphone>
            <enable>1</enable>
            <type>0</type>  <!-- 0 = usb2serial, 1 = ew11 -->
            <usb2serial>
                <port>/dev/ttyUSB0</port>
                <baud>3840</baud>
            </usb2serial>
            <ew11>
                <ipaddr>192.168.0.1</ipaddr>
                <port>8899</port>
            </ew11>
        </subphone>
    </rs485>
    <mqtt>
        <host>mosquitto ip address</host>
        <port>mosquitto port</port>
        <username>mosquitto auth id</username>
        <password>mosquitto auth password</password>
        <console_log>0</console_log>
        <verbose_regular_publish>0</verbose_regular_publish>
    </mqtt>
    <rooms>
        <room1>
            <name>Livingroom</name>
            <index>1</index>
            <lights>
                <light1>
                    <name>ceil 1</name>
                    <mqtt>
                        <publish>home/hillstate/light/state/1/1</publish>
                        <subscribe>home/hillstate/light/command/1/1</subscribe>
                    </mqtt>
                </light1>
                <light2>
                    <name>ceil 2</name>
